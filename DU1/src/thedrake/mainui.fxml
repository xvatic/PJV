<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thedrake.Controller">
   <children>
      <VBox layoutX="1.0" layoutY="304.0" prefHeight="495.0" prefWidth="1200.0" spacing="20.0" stylesheets="@style.css">
         <children>
            <Button fx:id="duelButton" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="185.0" text="1 vs 1">
               <VBox.margin>
                  <Insets />
               </VBox.margin></Button>
            <Button fx:id="duelAiButton" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="185.0" text="1 vs AI" />
            <Button fx:id="multiplayerButton" alignment="CENTER" mnemonicParsing="false" prefHeight="27.0" prefWidth="185.0" text="Multiplayer" />
            <Button fx:id="exitButton" alignment="CENTER" mnemonicParsing="false" prefHeight="19.0" prefWidth="185.0" text="Exit" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="200.0" left="508.0" />
         </padding></VBox>
      <ImageView fx:id="logo" fitHeight="255.0" fitWidth="352.0" layoutX="709.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pics/logo.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
